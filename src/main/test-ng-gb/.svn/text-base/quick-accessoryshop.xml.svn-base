<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

	<suite name="Eshop - Accessories page"	parallel="false">
	<listeners>
	<listener class-name="com.sonyericsson.automatedtests.Screenshot" />
	</listeners>
	<test name="Accessories Page testing -GB">
		<parameter name="locale" value="gb" />
		<parameter name="page" value="accessories" />
		<parameter name="browser" value="*firefox" />	
		<parameter name="javascriptRequired" value="true" />
		<groups>
			<run>
				<include name="quicktest" />
			</run>
		</groups>
		<classes>
			<class name="com.sonyericsson.automatedtests.quick.UsingAccessoryshopPage"></class>
			
		</classes>
	</test>
	
