<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Eshop - Phoneshop page" verbose="3"
	parallel="false" thread-count="10">
	
	<test name="Phoneshop page testing -GB">
		<parameter name="locale" value="gb" />
		<parameter name="page" value="phones" />
		<parameter name="browser" value="*firefox" />
		<parameter name="javascriptRequired" value="true" />
		<groups>
			<run>
				<include name="quicktest" />
			</run>
		</groups>
		<classes>
			<class name="com.sonyericsson.automatedtests.quick.UsingPhoneshopPage"></class>
		</classes>
	</test>
	
